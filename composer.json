{
    "name": "orlandocharles/cfdi",
    "description": "Librería para generar Comprobantes Fiscales para la versión 3.3",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cfdi", "sat", "invoice", "mexico"
    ],
    "authors": [
        {
            "name": "Orlando Charles",
            "email": "me@orlandocharles.com",
            "homepage": "http://orlandocharles.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "eclipxe/cfdiutils": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Charles\\CFDI\\": "src/CFDI"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Charles\\Tests\\CFDI\\": "tests/"
        }
    },
    "prefer-stable": true
}
